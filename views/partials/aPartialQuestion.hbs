
{{#each this}}
    {{#if @index}}
        <div class="question">
            <h4>{{@index}}. {{this.question}}</h4>
            <ol type="a">
            {{#each this.randomizedAnswers}}
                {{#if @index}}
                    <li><h5>{{this}}</h5></li>
                {{/if}}
            {{/each}}
            </ol>
            <div class="center-button">
                <div>
                    <a href="/editquestion?questionID={{this.id}}" class="pb-2 mb-2 btn btn-info" role="button">Editar</a>
                    <a href="/deletequestion?questionID={{this.id}}" class="pb-2 mb-2 btn btn-info" role="button">Deletar</a>
                </div>
            </div>
        </div>
    {{/if}}
{{/each}}