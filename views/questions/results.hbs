<div class="socorroJesus">

<h3 class="question answer text-center">Resultado do Teste</h3>
<ul class="answer">
  {{#each ordTestResults}}
    {{#if @index}}
      <li class="question">
        <h4>{{@index}}. {{this.[4]}}</h4>
        {{#if this.[1]}}
          <h3 class="text-success my-3"> Correto! </h3>
        {{else}}
          <h3 class="text-danger my-3"> Errado! </h3>
        {{/if}}
        <h4 class=""> Assinalado:</h4>
        <h5> {{this.[2]}} </h5>
        <h4 class="mt-3"> Resposta:</h4>
        <h5> {{this.[3]}} </h5>
        </li>
    {{/if}}
  {{/each}}
</ul>
</div>
